{
  "name": "@tekclaw/vix-utils",
  "version": "1.0.24",
  "description": "",
  "main": "dist",
  "typings": "./src/index.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./paths": {
      "types": "./dist/paths.d.ts",
      "default": "./dist/paths.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tekclaw/scaffold": "1.0.24",
    "chalk": "^4.1.2",
    "deepmerge": "^4.2.2",
    "vite": "latest"
  },
  "devDependencies": {
    "@types/node": "latest"
  },
  "peerDependencies": {
    "typescript": "^4"
  },
  "scripts": {
    "build": "rm -rf lib esm dist && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --build --force",
    "build:esm": "tsc --module es2015 --outDir esm",
    "watch": "tsc -p tsconfig.json -w",
    "lint": "eslint ./src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "gitHead": "ec70e62fe875ebc21d423ac9bd01fa6efd9a769b"
}
