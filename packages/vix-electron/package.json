{
  "name": "@tekclaw/vix-electron",
  "version": "1.0.27",
  "description": "",
  "main": "dist/index.js",
  "typings": "src/index.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf lib esm dist && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --build --force",
    "build:esm": "tsc --module es2015 --outDir esm"
  },
  "files": [
    "dist",
    "inject",
    "electron-main",
    "public",
    "src"
  ],
  "keywords": [
    "vite-plugin",
    "vite",
    "electron",
    "electron-builder"
  ],
  "devDependencies": {
    "@types/node": "latest",
    "electron": "^27.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "latest"
  },
  "dependencies": {
    "builtin-modules": "^3.3.0",
    "deepmerge": "^4.3.0",
    "electron-builder": "^24.6.0",
    "electron-devtools-installer": "^3.2.0",
    "esbuild": "^0.18.4",
    "fs-extra": "^11.1.0",
    "vite": "latest"
  }
}
